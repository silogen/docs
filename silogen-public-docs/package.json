{
  "name": "silogen-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "TINA_PUBLIC_IS_LOCAL=true tinacms dev --verbose -c \"next dev\"",
    "dev:prod": "TINA_PUBLIC_IS_LOCAL=false tinacms dev --verbose -c \"next dev\"",
    "build": "tinacms build  --verbose --partial-reindex && docusaurus build && next build",
    "docusaurus": "docusaurus",
    "docusaurus-start": "docusaurus start",
    "nextbuild": "next build",
    "tinacmsbuild": "tinacms build --verbose --skip-indexing",
    "docusaurusbuild": "docusaurus build",
    "start": "next start",
    "lint": "next lint --fix",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "pretty-quick --staged",
    "format": "npm run format:check || npm run format:fix",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "build-no-docusaurus": "tinacms build --verbose --preview-base-branch main --preview-name external-docs --skip-indexing && next build",
    "clean": "docusaurus clear && rm -r .next .yarn node_modules tina/__generated__",
    "prepare": "cd ../.. && husky docs/silogen-public-docs/.husky",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.5.2",
    "@docusaurus/tsconfig": "3.5.2",
    "@docusaurus/types": "3.5.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.7",
    "husky": "^9.1.6",
    "postcss": "^8.4.41",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5",
    "pretty-quick": "^4.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.2"
  },
  "dependencies": {
    "@docusaurus/core": "3.5.2",
    "@docusaurus/module-type-aliases": "3.5.2",
    "@docusaurus/preset-classic": "3.5.2",
    "@mdx-js/react": "^3.0.1",
    "@tinacms/cli": "^1.6.1",
    "@tinacms/datalayer": "^1.3.1",
    "clsx": "^2.1.1",
    "cookie": "^0.6.0",
    "mongodb-level": "^0.0.4",
    "next": "14.2.1",
    "next-auth": "^4.24.7",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tinacms": "^2.2.2",
    "tinacms-authjs": "^5.0.1",
    "tinacms-gitprovider-github": "^2.0.1",
    "title": "^3.5.3",
    "typescript": "^5.5.4",
    "vitest": "^2.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
